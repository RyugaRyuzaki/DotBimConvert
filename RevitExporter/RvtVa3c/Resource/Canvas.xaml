<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="CanvasRebar" TargetType="{x:Type Canvas}">
        <Setter Property="Width" Value="140"></Setter>
        <Setter Property="Height" Value="60"></Setter>
    </Style>
    <Style x:Key="Up" TargetType="{x:Type Line}">
        <Setter Property="X1" Value="10"></Setter>
        <Setter Property="X2" Value="130"></Setter>
        <Setter Property="Y1" Value="50"></Setter>
        <Setter Property="Y2" Value="50"></Setter>
        <Setter Property="Stroke" Value="Red"></Setter>
        <Setter Property="StrokeThickness" Value="2"></Setter>
    </Style>
    <Style x:Key="Down" TargetType="{x:Type Line}">
        <Setter Property="X1" Value="10"></Setter>
        <Setter Property="X2" Value="130"></Setter>
        <Setter Property="Y1" Value="10"></Setter>
        <Setter Property="Y2" Value="10"></Setter>
        <Setter Property="Stroke" Value="Red"></Setter>
        <Setter Property="StrokeThickness" Value="2"></Setter>
    </Style>
    <Style x:Key="Left" TargetType="{x:Type Line}">
        <Setter Property="X1" Value="10"></Setter>
        <Setter Property="X2" Value="10"></Setter>
        <Setter Property="Y1" Value="10"></Setter>
        <Setter Property="Y2" Value="50"></Setter>
        <Setter Property="Stroke" Value="Red"></Setter>
        <Setter Property="StrokeThickness" Value="2"></Setter>
    </Style>
    <Style x:Key="Right" TargetType="{x:Type Line}">
        <Setter Property="X1" Value="130"></Setter>
        <Setter Property="X2" Value="130"></Setter>
        <Setter Property="Y1" Value="10"></Setter>
        <Setter Property="Y2" Value="50"></Setter>
        <Setter Property="Stroke" Value="Red"></Setter>
        <Setter Property="StrokeThickness" Value="2"></Setter>
    </Style>
</ResourceDictionary>